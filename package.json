{
  "name": "block-outliner",
  "displayName": "Block Outliner",
  "description": "BlockOutliner visually highlights code blocks enclosed by parentheses, brackets, and braces, enhancing readability and easing navigation through nested structures.",
  "icon": "img/block-outliner.png",
  "version": "1.0.0",
  "license": "MIT",
  "publisher": "magnomatos822",
  "author": {
    "name": "Magno Matos",
    "url": "https://github.com/magnomatos822",
    "email": "magnomatos822@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magnomatos822/vscode-block-outliner.git"
  },
  "homepage": "https://github.com/magnomatos822/vscode-block-outliner",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "block",
    "outliner"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "block-outliner.blockOutliner",
        "title": "Block Outliner: Block Outliner"
      }
    ],
    "keybindings": [
      {
        "command": "block-outliner.blockOutliner",
        "key": "ctrl+alt+b"
      }
    ],
    "languages": [
      {
        "id": "*",
        "extensions": [
          "*"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}